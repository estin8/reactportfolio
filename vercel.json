{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src https://nates.vercel.app; script-src 'self' https://nates.vercel.app; img-src 'self' https://nates.vercel.app"
                }
            ]
        }
    ]
}